@{
    ViewBag.Title = "About";
}






<!-- Navbar & Hero Start -->
<div class="container-fluid position-relative p-0">
    <!-- Header Start -->
    <div class="container-fluid bg-breadcrumb" style=" background: linear-gradient(rgba(0, 0, 0, 0.7), rgba(0, 0, 0, 0.7)), url(../Content/front/img/carousel-1.jpg); ">
        <div class="container text-center py-5" style="max-width: 900px;">
            <h4 class="text-white display-4 mb-4 wow fadeInDown" data-wow-delay="0.1s">About Us</h4>
        </div>
    </div>
    <!-- Header End -->
</div>
<!-- Navbar & Hero End -->
<!--/w3l-midslider-->
<section class="w3l-midslider py-5" id="midslider">
    <!-- /grids -->
    <div class="container py-md-5">
        <div class="row">
            <div class="col-lg-5 title-content text-left pe-lg-5 mb-lg-0 mb-5">
                <h3 class="text-primary">
                    Work Code
                </h3>
                <p class="mb-3">

                    WorkCode is your trusted partner in the digital realm, offering a comprehensive suite of services to propel your business forward. From programming and development to research, marketing, and page management, we provide tailored solutions designed to meet your unique needs and drive success. With our expertise, innovation, and client-centric approach, we empower businesses to thrive in today's fast-paced digital landscape. Let's craft your digital success story together with WorkCode.
                </p>
                <ul class="icon-list-items mt-3">
                    <li class="icon-list-item">
                        <i aria-hidden="true" class="fas fa-check"></i>
                        Innovative ideas
                    </li>


                </ul>

            </div>
            <!-- /grids -->
            <div class="col-lg-7 stats_info-images ps-lg-5">
                <div class="stats_info-images-right row">
                    <img src="~/Content/front/img/workcode.jpg" class="img-fluid radius-image" alt="" />


                </div>
            </div>

        </div>
        <!-- /grids -->
    </div>
</section>
<!--//w3l-midslider-->
<!--/w3l-project-->
<section class="w3l-index5 py-5" style="background: url(../Content/front/img/banner2.jpg) no-repeat center; background-size: cover; box-shadow:20%">
    <div class="container py-md-3" >
        <div class="w3l-project-in">
            <div class="row">
                <div class="col-lg-7">
                    <div class="bottom-info">
                        <div class="header-section pe-lg-5">
                            <h3 class="title-w3l two mb-3" style="color:white">
                                Get Started Today
                            </h3>
                            <p style="color:white">
                                Ready to take your business to new heights? Contact us today to learn more about our services and how we can help you achieve your digital goals. Whether you're a small startup or a large enterprise, we have the expertise and resources to drive your success. Let's embark on this journey together and forge a brighter future for your business with WorkCode.
                            </p>
                        </div>
                    </div>
                </div>

                <div class="col-lg-5 align-self mt-lg-0 mt-sm-5 mt-4">
                    <div class="w3l-buttons d-sm-flex justify-content-end">
                        <a href="@Url.Action("Let's Work to Gether", "Home")" class="btn btn-style btn-primary">Let's Work to Gether  </a>
                        <a href="@Url.Action("Create", "Contact")" class="btn btn-style btn-outline-primary">  Contact <i class="fas fa-arrow-right ms-lg-4 ms-2"></i> </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!--//w3l-project-->
